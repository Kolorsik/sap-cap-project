{
  "name": "helloworld",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^5",
    "express": "^4",
    "passport": "^0.4.1",
    "@sap/hana-client": "^2.6.61",
    "@sap/xssec": "^3.2.2",
    "@sap/audit-logging": "^5.0.0"
  },
  "devDependencies": {
    "jest": "^27.0.6",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.3"
  },
  "scripts": {
    "start": "cds run",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es2020": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "globals": {
      "SELECT": true,
      "INSERT": true,
      "UPDATE": true,
      "DELETE": true,
      "CREATE": true,
      "DROP": true,
      "CDL": true,
      "CQL": true,
      "CXL": true,
      "cds": true
    },
    "rules": {
      "no-console": "off",
      "require-atomic-updates": "off"
    }
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "credentials": {
          "database": "phoneshop.db"
        },
        "model": [
          "db",
          "srv"
        ]
      },
      "API_BUSINESS_PARTNER": {
        "kind": "odata",
        "model": "srv\\external\\API_BUSINESS_PARTNER"
      }
    },
    "hana": {
      "deploy-format": "hdbtable"
    },
    "uaa": {
      "kind": "xsuaa"
    },
    "messaging": {
        "kind": "file-based-messaging"
    }
  }
}